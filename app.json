{
  "expo": {
    "name": "HFB-Mobile",
    "slug": "HFB-Mobile",
    "privacy": "unlisted",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/HFB-icon-60x60.png",
    "splash": {
      "image": "./assets/HomeForeverBaths-Logo--blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homeforeverbaths.hfbmobile",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses faceId to locally authenticate that the user can access the application."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Mobile application for Home Forever Baths to help with contract creation and order accuracy for customers"
  }
}
